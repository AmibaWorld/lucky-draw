/* pages/custom/custom.wxss */
page{
     background-color: "#7f7f7f";
     color:white;
}

#display-container{
    height: 80rpx;
    width: 100%;
    border-top:1px solid white;
    border-bottom:1px solid white;
    position:absolute;
    top:15%;
    text-align: center;
    line-height: 80rpx;
}

.btn-container{
    width:200rpx;
    height: 100rpx;
    position: absolute;
    left: 50%;
    margin-left: -100rpx;
    bottom: 30%;
    border: 1px solid white;
    border-radius: 30rpx;
    text-align: center;
    line-height: 100rpx;
}
